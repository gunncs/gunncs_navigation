<launch>

  <!--- Run the kinect -->
  <include file="$(find gunncs_navigation_2dnav)/launch/kinect.launch" />

  <!-- Run the map server -->
  <arg name="map_file" default="$(find gunncs_navigation_2dnav)/maps/cubicle.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!--- Run AMCL -->
  <include file="$(find gunncs_navigation_2dnav)/config/amcl_turtlebot.launch" />

  <!--- Run Move Base -->
  <include file="$(find gunncs_navigation_2dnav)/config/move_base_turtlebot.launch" />

</launch>
